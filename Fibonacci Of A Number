<!DOCTYPE html>
<html>
<head>
<title>Factorial Program</title>
</head>
<body>
<h2>Factorial of a number</h2>
<input type="number" id="num">
<button onclick="findFabonicci()">Generate</button>
<p id="output"></p>
<script>
function findFabonicci(){
let n=document.getElementById("num").value;
let a=0,b=1,c;
let result=a+" "+b+" ";
for(let i=2;i<n;i++)
{
c=a+b;
result+=c+" ";
a=b;
b=c;
}
document.getElementById("output").innerHTML==result;
}
</script>
</body>
</html>
